{
  "name": "@aaronghx/eslint-config",
  "version": "0.1.4",
  "description": "eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "license": "MIT",
  "author": "AaronGuo <1359774872@qq.com> (https://github.com/HongxuanG)",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --config=index.js",
    "lint:fix": "eslint . --fix --config=index.js",
    "publish:major": "npm version major && pnpm publish --no-git-checks",
    "publish:minor": "npm version minor && pnpm publish --no-git-checks",
    "publish:patch": "npm version patch && pnpm publish --no-git-checks"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0"
  },
  "dependencies": {
    "@aaronghx/eslint-config-basic": "workspace:^",
    "@aaronghx/eslint-config-react": "workspace:^",
    "@aaronghx/eslint-config-ts": "workspace:^",
    "@aaronghx/eslint-config-vue": "workspace:^",
    "@aaronghx/eslint-config-wx": "workspace:^"
  },
  "devDependencies": {
    "eslint": "^8.13.0"
  }
}
