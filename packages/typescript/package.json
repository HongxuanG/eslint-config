{
  "name": "@aaronghx/eslint-config-ts",
  "version": "0.1.4",
  "description": "eslint-config ts模块",
  "keywords": [
    "eslint-config-ts"
  ],
  "license": "MIT",
  "author": "AaronGuo <1359774872@qq.com> (https://github.com/HongxuanG)",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --config=index.js",
    "lint:fix": "eslint . --fix --config=index.js",
    "publish:major": "npm version major && pnpm publish --no-git-checks",
    "publish:minor": "npm version minor && pnpm publish --no-git-checks",
    "publish:patch": "npm version patch && pnpm publish --no-git-checks"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "typescript": ">=3.9"
  },
  "dependencies": {
    "@aaronghx/eslint-config-basic": "workspace:^0.1.4",
    "@aaronghx/eslint-config-ts": "workspace:^0.1.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0"
  },
  "devDependencies": {
    "eslint": "^8.10.0"
  }
}
